host.get
 - output
 - filter:
	 - hostids
	 - groupids
	 - maintenance
	 - status:          any/monitored/notMonitored
	 - host
	 - hasItems:        any/monitored/historical/simpleGraph
	 - hasTriggers:     any/monitored
	 - hasHttpTests:    any/monitored
	 - hasGraphs:       any
	 - hasApplications: any
 - permission:      read/write
 - sort
 - limit

trigger.get
 - output
 - filter:
    - triggerids
    - description
    - status:               any/inherited/templated/monitored/active
    - inheritedFromTemplate
 - permission:              read/write
 - sort
 - limit